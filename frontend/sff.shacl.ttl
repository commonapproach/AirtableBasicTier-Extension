        sh:property     csh:servesEDG_PropertyShape , csh:sectorServed_PropertyShape , csh:reportedDate_PropertyShape , csh:provinceTerritoryServed_PropertyShape , csh:primaryPopulationServed_PropertyShape , csh:organizationType_PropertyShape , csh:localityServed_PropertyShape , csh:hasPrimaryContact_PropertyShape , csh:hasManagementTeamProfile_PropertyShape , csh:hasFundingStatus_PropertyShape , csh:hasBoardProfile_PropertyShape;
