csh:hasTeamSize_PropertyShape
        rdf:type         sh:PropertyShape;
        sh:datatype      xsd:string;
        sh:maxCount      1;
        sh:minCount      0;
        sh:name          "hasTeamSize";
        sh:nodeKind      sh:Literal;
        sh:path          sff:hasTeamSize;
        csh:cardinality  csh:Optional;
        csh:usedBy       sff:TeamProfile .

